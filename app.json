{
  "expo": {
    "name": "ChatApp",
    "slug": "ChatApp",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/myIcons/medium512.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/myIcons/medium512.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chatapp.myapp"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/myIcons/medium512.png",
        "backgroundColor": "#000000"
      },
      "package": "com.chatapp.myapp",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a22bfaf8-e21d-4cb8-b6b0-d5b80eece243"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/myIcons/medium512.png",
          "color": "black"
        }
      ]
    ]
  }
}
